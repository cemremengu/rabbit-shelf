      <dependency id="Autofac" version="4.2.0" />
      <dependency id="EasyNetQ" version="0.63.5.454" />
      <dependency id="Topshelf" version="4.0.3" />
    </dependencies>
  </metadata>

  <files>
    <file src="build\RabbitShelf.dll" target="lib\net45" />
    <file src="build\RabbitShelf.pdb" target="lib\net45" />

  </files>
